		{
			"name": "Create user",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Successful POST request\", function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([201, 202]);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Status code name has string 'Created'\", () => {\r",
							"  pm.response.to.have.status(\"Created\");\r",
							"});\r",
							"\r",
							"pm.test(\"Content-Type header is present\", () => {\r",
							"  pm.response.to.have.header(\"Content-Type\");\r",
							"});\r",
							"\r",
							"const jsonData = pm.response.json();\r",
							"pm.test(\"Test data type of the response\", () => {\r",
							"  pm.expect(jsonData).to.be.an(\"object\");\r",
							"  pm.expect(jsonData.job).to.be.a(\"string\");\r",
							"  pm.expect(jsonData.id).to.be.a(\"string\");\r",
							"  //pm.expect(jsonData.website).to.be.undefined;\r",
							"  //pm.expect(jsonData.email).to.be.null;\r",
							"  //pm.expect(jsonData.hobbies).to.be.an(\"array\");\r",
							"  //pm.expect(jsonData.id).to.be.a(\"number\");\r",
							"});"
						],
